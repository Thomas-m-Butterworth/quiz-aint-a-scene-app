{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@screens/*": ["src/screens/*"],
      "@utils/*": ["src/utils/*"],
      "@navigators/*": ["src/navigators/*"],
      "@assets/*": ["assets/*"]
    }
  },
  "include": ["src", "images.d.ts"],
  "exclude": ["node_modules"]
}
